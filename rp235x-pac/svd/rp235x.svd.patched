    <peripheral>
      <name>XOSC</name>
      <version>1</version>
      <description>Controls the crystal oscillator</description>
      <baseAddress>0x40048000</baseAddress>
      <size>0x20</size>
      <addressBlock>
        <offset>0x0</offset>
        <size>0x1000</size>
